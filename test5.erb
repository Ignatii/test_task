class Dessert
  attr_accessor :name, :calories

  def initialize(name, calories)
    @name = name
    @calories = calories
  end

  def healty?

    @calories < 200 ? true: false

  end

  def delicious?

    @name == "dessert" ? true: false

  end

end

test = Dessert.new("dessert",400)

puts test.healty?
puts test.delicious?